{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean-theme": "cd themes/tranquilpeak; git checkout .",
    "build-theme": "npm run clean-theme; git submodule update; cd themes/tranquilpeak; npm i; bower i; grunt build buildProd",
    "build": "cp -f tranquilpeak_config.yml themes/tranquilpeak/_config.yml && hexo generate"
  },
  "hexo": {
    "version": "3.1.1"
  },
  "dependencies": {
    "hexo": "^3.1.0",
    "hexo-admin": "^1.1.1",
    "hexo-deployer-git": "0.0.4",
    "hexo-generator-archive": "^0.1.2",
    "hexo-generator-category": "^0.1.2",
    "hexo-generator-feed": "^1.0.3",
    "hexo-generator-ghpages": "^1.0.1",
    "hexo-generator-index": "^0.1.2",
    "hexo-generator-tag": "^0.1.1",
    "hexo-renderer-ejs": "^0.1.0",
    "hexo-renderer-markdown-it": "^3.4.0",
    "hexo-renderer-stylus": "^0.3.0",
    "hexo-server": "^0.1.2"
  }
}
